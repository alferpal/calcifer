  "license": "Parity-7.0.0",
  "dependencies": {
    "@alferpal/calcifer-utils": "~0.0.0",
    "@hapi/boom": "~9.1.1"
  },
  "devDependencies": {
    "@alferpal/calcifer-types": "~0.0.0",
    "@alferpal/calcifer-typings": "~0.0.0",
    "@alferpal/eslint-config-calcifer": "~0.0.0",
